<configuration>
    <property>
        <name>ockb_user</name>
        <display-name>Knowledge Base User</display-name>
        <value>ockb</value>
        <property-type>USER</property-type>
        <description>User to run Knowledge Base service</description>
    </property>
    <property>
        <name>ockb_user_group</name>
        <display-name>Knowledge Base User Group</display-name>
        <value>ockb</value>
        <property-type>GROUP</property-type>
        <description></description>
    </property>
    <property>
        <name>ockb_pid_dir</name>
        <value>/var/run/ockb</value>
        <description>Knowledge Base PID Dir</description>
        <value-attributes>
            <type>directory</type>
            <overridable>false</overridable>
            <editable-only-at-install>true</editable-only-at-install>
        </value-attributes>
    </property>
</configuration>